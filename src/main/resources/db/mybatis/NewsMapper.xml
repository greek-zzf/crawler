        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="db.NewsMapper">
    <select id="selectBlog" resultType="Blog">
        select link
        from LINKS_TO_BE_PROCESSED
        limit 1;
